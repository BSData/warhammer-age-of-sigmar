        <profile id="1874-7b7b-c792-fd58" name="Splinter-screech" hidden="false" typeId="2e81-5e22-c6e1-73cb" typeName="Spell">
          <characteristics>
            <characteristic name="Casting Value" typeId="2508-b604-1258-a920">5</characteristic>
            <characteristic name="Description" typeId="76ff-781d-b8e6-5f27">If successfully cast, pick 1 enemy model within 12&quot; of the caster, and roll a dice. If the roll is equal to or greater than that model’s Wounds characteristic, it is slain.</characteristic>
          </characteristics>
        </profile>
        <profile id="3a63-637e-e109-fffe" name="Protection of the Horned Rat" hidden="false" typeId="c924-5a68-471a-2fd5" typeName="Unit Abilities">
          <characteristics>
            <characteristic name="Ability Details" typeId="d4dc-8e81-bc0e-b8f0">Roll a dice each time you allocate a wound or mortal wound to this model. On a 5+ that wound or mortal wound is negated.</characteristic>
          </characteristics>
        </profile>
        <profile id="12bd-f4cf-7393-a1d3" name="Terrifying" hidden="false" typeId="c924-5a68-471a-2fd5" typeName="Unit Abilities">
