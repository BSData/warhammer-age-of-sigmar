            </selectionEntry>
            <selectionEntry id="9f7a-90c4-8468-4f33" name="The Grieving Legion" hidden="false" collective="false" import="true" type="upgrade">
              <profiles>
                <profile id="a21a-aeaa-8b15-7f46" name="Dragged into the Grave" hidden="false" typeId="c137-4d1f-9d1a-524d" typeName="Battle Trait">
                  <characteristics>
                    <characteristic name="Battle Trait Details" typeId="9fdd-b4b1-5f7a-0970">Enemy units cannot retreat while they are within 3&quot; of any friendly GRIEVING LEGION units with 10 or more models.</characteristic>
                  </characteristics>
                </profile>
              </profiles>
              <costs>
                <cost name="pts" typeId="points" value="0"/>
