            </profile>
          </profiles>
          <costs>
            <cost name="pts" typeId="points" value="0.0"/>
          </costs>
        </selectionEntry>
        <selectionEntry id="aca3-8115-9073-6a86" name="3. Spectral Grasp" hidden="false" collective="false" import="true" type="upgrade">
