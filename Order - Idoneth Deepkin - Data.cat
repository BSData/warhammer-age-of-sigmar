        <selectionEntry id="0af0-ed65-632c-f422" name="5. Disharmony Stones" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" id="cd8d-5152-c565-b831" type="max"/>
