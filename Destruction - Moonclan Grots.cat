          <conditions>
            <condition field="selections" scope="e82f-5e75-3047-d0d3" value="0.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" childId="681a-02ca-afc8-aa1e" type="greaterThan"/>
          </conditions>
