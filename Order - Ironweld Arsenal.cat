          <rules/>
          <infoLinks/>
          <modifiers/>
          <characteristics>
            <characteristic name="Ability Details" characteristicTypeId="d4dc-8e81-bc0e-b8f0" value="You can re-roll the dice rolled to see if an Organ Gun jams if there is an Engineer from your army within 1&quot; of the war machine."/>
          </characteristics>
        </profile>
