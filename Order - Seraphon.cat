      <selectionEntries>
        <selectionEntry id="42d9-2fe7-41d2-6e0c" name="1. Zoetic Dial" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" id="70a1-ed45-4329-7b1d" type="max"/>
