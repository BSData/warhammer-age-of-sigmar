          </costs>
        </selectionEntry>
        <selectionEntry id="89db-6a87-6617-8fe6" name="2. Walk Between Realms" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="efb7-16a1-f17b-a68d" type="max"/>
