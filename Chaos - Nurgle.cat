            <profile id="d8a8-739b-07d9-a2d0" name="Resilient" hidden="false" typeId="c749-bae4-71a8-0c36" typeName="Command Trait">
              <characteristics>
                <characteristic name="Command Trait Details" typeId="ee96-6f3a-e5ca-2350">Roll a dice each time you allocate a wound or mortal wound to your general. On a 6+ the wound is negated.</characteristic>
              </characteristics>
            </profile>
          </profiles>
          <costs>
