                  <selectionEntries>
                    <selectionEntry id="4f9f-9866-5a67-76b9" name="Allherd" hidden="false" collective="false" import="true" type="upgrade">
                      <profiles>
                        <profile id="3507-f63b-153d-c352" name="Booming Roar" hidden="false" typeId="f71f-b0a4-730e-ced3" typeName="Command Abilities">
                          <characteristics>
                            <characteristic name="Command Ability Details" typeId="1b71-4c83-4e8c-093f">You can use this command ability at the start of your hero phase if your general is on the battlefield. If you do so, you receive 1 Primordial Call point.</characteristic>
                          </characteristics>
                        </profile>
                        <profile id="817e-6396-8143-28aa" name="Bestial Might" hidden="false" typeId="c137-4d1f-9d1a-524d" typeName="Battle Trait">
                          <characteristics>
                            <characteristic name="Battle Trait Details" typeId="9fdd-b4b1-5f7a-0970">Subtract 1 from battleshock rolls made for ALLHERD units in the battleshock phase if they were picked to fight in the combat phase of the same turn.</characteristic>
