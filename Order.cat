          <rules/>
          <infoLinks/>
          <modifiers/>
          <characteristics>
            <characteristic name="Ability Details" characteristicTypeId="d4dc-8e81-bc0e-b8f0" value="A Carnosaur that tastes the flesh of the enemy becomes a rampaging force of destruction. Once this model has clain an enemy with its Massive Jaws, it can run and charge in the same turn for the rest of the battle."/>
          </characteristics>
        </profile>
        <profile id="e4f9-b283-4fa1-4805" name="Bloodroar" hidden="false" profileTypeId="c924-5a68-471a-2fd5" profileTypeName="08 Ability (Unit)">
