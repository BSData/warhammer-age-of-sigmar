          <rules/>
          <infoLinks/>
          <modifiers/>
          <characteristics>
            <characteristic name="Ability Details" characteristicTypeId="d4dc-8e81-bc0e-b8f0" value="If any models flee from a unit within 10&quot; of Be’lakor, he is reinvigorated by their suffering and immediately heals D3 wounds."/>
          </characteristics>
        </profile>
        <profile id="275d-4604-aa36-2562" name="Enfeeble Foe" hidden="false" profileTypeId="2e81-5e22-c6e1-73cb" profileTypeName="05 Spell">
