          <rules/>
          <infoLinks/>
          <modifiers/>
          <characteristics>
            <characteristic name="Ability Details" characteristicTypeId="d4dc-8e81-bc0e-b8f0" value="Add 1 to the number of models that flee from enemy units that fail a battleshock test while they are within 12&quot; of this model."/>
          </characteristics>
        </profile>
        <profile id="2ffa-80f0-0e79-0782" name="Wail of the Damned" hidden="false" profileTypeId="c924-5a68-471a-2fd5" profileTypeName="Unit Abilities">
