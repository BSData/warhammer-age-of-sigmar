          <rules/>
          <infoLinks/>
          <modifiers/>
          <characteristics>
            <characteristic name="Ability Details" characteristicTypeId="d4dc-8e81-bc0e-b8f0" value="At the end of any combat phase in which Prince Vhordrai slew any enemy models, you can heal 1 wound that has been allocated tohim."/>
