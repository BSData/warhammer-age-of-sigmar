            <characteristic name="Cast/Unbind" typeId="8294-f605-2c0f-8f92">1</characteristic>
            <characteristic name="Spells Known" typeId="dc9c-47d3-6931-859c">Arcane Bolt, Mystic Shield, Sneaky Stabbin&apos;</characteristic>
