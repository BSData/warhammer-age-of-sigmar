    <profile id="02c3-af2c-b9ec-57ab" name="Banner Bearer" hidden="false" typeId="c924-5a68-471a-2fd5" typeName="Unit Abilities">
      <characteristics>
        <characteristic name="Ability Details" typeId="d4dc-8e81-bc0e-b8f0">Models in this unit can be Banner Bearers. You can re-roll battleshock tests for a unit that includes any Banner Bearers if it made a charge move in the same turn.</characteristic>
      </characteristics>
    </profile>
    <profile id="fc59-a01b-ea1b-f382" name="Trumpeter or Lutist" hidden="false" typeId="c924-5a68-471a-2fd5" typeName="Unit Abilities">
      <characteristics>
        <characteristic name="Ability Details" typeId="d4dc-8e81-bc0e-b8f0">Models in this unit can be Trumpeters or Lutists. Roll 3D6 instead of 2D6 when you make a charge roll for a unit that includes any Trumpeters or Lutists, and then pick two of the dice to determine the result of the roll.</characteristic>
      </characteristics>
    </profile>
    <profile id="f896-b95c-82e0-9ed0" name="Drummer" hidden="false" typeId="c924-5a68-471a-2fd5" typeName="Unit Abilities">
      <characteristics>
        <characteristic name="Ability Details" typeId="d4dc-8e81-bc0e-b8f0">Models in this unit can be Drummers. Add 1 to charge rolls for this unit if it includes any Drummers. </characteristic>
      </characteristics>
    </profile>
    <profile id="b266-188b-8b98-925e" name="Trumpeter (Peasantry)" hidden="false" typeId="c924-5a68-471a-2fd5" typeName="Unit Abilities">
      <characteristics>
        <characteristic name="Ability Details" typeId="d4dc-8e81-bc0e-b8f0">Models in this unit can be Trumpeters. Add 1 to run rolls for this unit if it includes any Trumpeters.</characteristic>
      </characteristics>
    </profile>
    <profile id="d9c6-1988-1c9c-01cf" name="Knight&apos;s Shield" hidden="false" typeId="c924-5a68-471a-2fd5" typeName="Unit Abilities">
      <characteristics>
        <characteristic name="Ability Details" typeId="d4dc-8e81-bc0e-b8f0">In the combat phase, re-roll save rolls of 1 for this unit if it made a charge move in the same turn.</characteristic>
      </characteristics>
    </profile>
    <profile id="78da-47ae-352c-551c" name="Standard Bearer" hidden="false" typeId="c924-5a68-471a-2fd5" typeName="Unit Abilities">
