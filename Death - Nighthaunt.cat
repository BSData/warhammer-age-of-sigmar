    <selectionEntry id="5683-d50c-009c-1a92" name="Black Coach" page="" hidden="false" collective="false" import="true" type="unit">
      <profiles>
        <profile id="79a1-b3f0-5d8e-9651" name="Black Coach" hidden="false" typeId="1960-ca8e-67ce-2014" typeName="Unit">
          <characteristics>
            <characteristic name="Move" typeId="8655-6213-2824-1752">10&quot;</characteristic>
            <characteristic name="Wounds" typeId="cd0e-fea6-411f-904d">12</characteristic>
