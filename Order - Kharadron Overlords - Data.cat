      <profiles>
        <profile id="c9a1-bd1d-0517-80bb" name="Aetheric Navigator" hidden="false" typeId="1960-ca8e-67ce-2014" typeName="Unit">
          <characteristics>
            <characteristic name="Move" typeId="8655-6213-2824-1752">4&quot;</characteristic>
