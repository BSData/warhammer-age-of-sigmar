    <selectionEntry id="129e-f84a-5017-ca90" name="Aether-Khemist" publicationId="e23c-3deb-pubN65537" hidden="false" collective="false" type="unit">
      <profiles>
        <profile id="2e83-3b67-430d-36db" name="Aether-Khemist" hidden="false" typeId="1960-ca8e-67ce-2014" typeName="Unit">
          <characteristics>
            <characteristic name="Move" typeId="8655-6213-2824-1752">4&quot;</characteristic>
