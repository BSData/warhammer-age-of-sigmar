        <profile id="5b5a-f0af-6ced-f06e" name="Deluge of Nurgle" hidden="false" typeId="2e81-5e22-c6e1-73cb" typeName="Spell">
          <characteristics>
            <characteristic name="Casting Value" typeId="2508-b604-1258-a920">7</characteristic>
