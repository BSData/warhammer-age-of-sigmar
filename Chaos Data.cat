        <selectionEntry id="d115-7692-e812-cd2b" name="2. Thrill-seeker" hidden="false" collective="false" type="upgrade">
          <profiles>
            <profile id="eaa0-486f-0038-0d16" name="Thrill-seeker" hidden="false" typeId="f71f-b0a4-730e-ced3" typeName="Command Abilities">
              <characteristics>
                <characteristic name="Command Ability Details" typeId="1b71-4c83-4e8c-093f">This general can run and still charge later in the same turn. If this general already has an ability that allows them to run and still charge later in the same turn, add 3 to run rolls for this general instead.</characteristic>
              </characteristics>
            </profile>
          </profiles>
          <costs>
            <cost name="pts" typeId="points" value="0.0"/>
          </costs>
        </selectionEntry>
        <selectionEntry id="3d44-ebe5-b31c-bda5" name="4. Trail-sniffer" hidden="false" collective="false" type="upgrade">
          <profiles>
            <profile id="6691-73ea-3717-3444" name="Trail-sniffer" hidden="false" typeId="c749-bae4-71a8-0c36" typeName="Command Trait">
              <characteristics>
                <characteristic name="Command Trait Details" typeId="ee96-6f3a-e5ca-2350">At the start of your hero phase, roll a dice for this general if this general is wholly within enemy territory. On a 3+, add 1 to the Attacks characteristic of this general&apos;s melee weapons until your next hero phase.</characteristic>
              </characteristics>
            </profile>
          </profiles>
          <costs>
            <cost name="pts" typeId="points" value="0.0"/>
          </costs>
        </selectionEntry>
        <selectionEntry id="05c1-bf23-261d-36d1" name="6. Speed-chaser" hidden="false" collective="false" type="upgrade">
          <profiles>
            <profile id="1663-3361-7ed7-c7d4" name="Speed-chaser" hidden="false" typeId="c749-bae4-71a8-0c36" typeName="Command Trait">
              <characteristics>
                <characteristic name="Command Trait Details" typeId="ee96-6f3a-e5ca-2350">This general can retreat and still charge later in the same turn.</characteristic>
              </characteristics>
            </profile>
          </profiles>
          <costs>
            <cost name="pts" typeId="points" value="0.0"/>
          </costs>
        </selectionEntry>
        <selectionEntry id="71db-ac4d-07e5-73fa" name="5. Symphoniac" hidden="false" collective="false" type="upgrade">
          <profiles>
            <profile id="df3a-1cfa-f336-17cb" name="Symphoniac" hidden="false" typeId="c749-bae4-71a8-0c36" typeName="Command Trait">
              <characteristics>
                <characteristic name="Command Trait Details" typeId="ee96-6f3a-e5ca-2350">At the start of the combat phase, roll 1 dice for each enemy unit within 3&quot; of this general. On a 2+ that enemy unit suffers 1 mortal wound.</characteristic>
              </characteristics>
            </profile>
          </profiles>
          <costs>
            <cost name="pts" typeId="points" value="0.0"/>
          </costs>
        </selectionEntry>
        <selectionEntry id="993e-3a73-60c3-64fb" name="3. Into the Fray" hidden="false" collective="false" type="upgrade">
          <profiles>
            <profile id="b1c4-5434-ee81-3ae6" name="Into the Fray" hidden="false" typeId="c749-bae4-71a8-0c36" typeName="Command Trait">
              <characteristics>
                <characteristic name="Command Trait Details" typeId="ee96-6f3a-e5ca-2350">The hit roll for the first attack made by this general during the battle is automatically a 6 (do not roll the dice).</characteristic>
              </characteristics>
            </profile>
          </profiles>
          <costs>
            <cost name="pts" typeId="points" value="0.0"/>
          </costs>
        </selectionEntry>
