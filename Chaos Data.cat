      <selectionEntries>
        <selectionEntry id="06d4-2624-e2c9-f142" name="1. Hunter Supreme" hidden="false" collective="false" type="upgrade">
          <profiles>
            <profile id="c091-41b7-3708-e1ca" name="Hunter Supreme" hidden="false" typeId="f71f-b0a4-730e-ced3" typeName="Command Abilities">
              <characteristics>
                <characteristic name="Command Ability Details" typeId="1b71-4c83-4e8c-093f">You can re-roll hit and wound rolls of 1 for attacks made by this general if this general made a charge move in the same turn.</characteristic>
              </characteristics>
            </profile>
          </profiles>
          <costs>
            <cost name="pts" typeId="points" value="0.0"/>
