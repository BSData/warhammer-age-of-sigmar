    <selectionEntry id="a620-1f6e-e451-6d30" name="Allegiance: Legion of the First Prince" publicationId="9ba2-cdb6-f9ce-98d0" hidden="false" collective="false" import="true" type="upgrade">
      <constraints>
        <constraint field="selections" scope="parent" value="1" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" id="94d3-7110-de2b-0110" type="max"/>
