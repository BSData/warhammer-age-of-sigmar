        <selectionEntry id="34e9-a3c0-cb93-38bc" name="5. Aspect of Tzeentch" hidden="false" collective="false" type="upgrade">
          <constraints>
            <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" id="7d88-a199-b80c-c6f5" type="max"/>
          </constraints>
          <profiles>
            <profile id="7082-77b0-b240-9b89" name="Aspect of Tzeentch" hidden="false" typeId="0ac4-aacb-2481-8e72" typeName="Artefact">
              <characteristics>
                <characteristic name="Artefact Details" typeId="0918-c47a-d84e-c0cf">Each time you expend a Destiny Dice whilst this daemon is on the battlefield, roll a dice. On a 6 you may immediately roll another dice and add it to your Destiny Dice pool.</characteristic>
              </characteristics>
            </profile>
          </profiles>
          <costs>
            <cost name="pts" typeId="points" value="0.0"/>
          </costs>
        </selectionEntry>
        <selectionEntry id="350e-b5ec-6b7e-5ca8" name="6. Mark of the Conjurer" hidden="false" collective="false" type="upgrade">
