                            <characteristic name="Battle Trait Details" typeId="9fdd-b4b1-5f7a-0970">Add 1 to hit rolls for Draichi Ganeth units in the combat phase if they charged in the same turn.</characteristic>
                          </characteristics>
                        </profile>
                      </profiles>
                      <rules>
                        <rule id="dc14-0a41-bccf-ee14" name="WARSCROLL BATTALIONS" hidden="false">
