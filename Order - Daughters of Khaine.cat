        <selectionEntry id="19ce-7158-4f88-32d8" name="Morathi, The Shadow Queen" book="Order Battletome: Daughters of Khaine" hidden="false" collective="false" type="model">
          <profiles>
            <profile id="6f22-05d1-2f2f-2ab5" name="Morathi, The Shadow Queen" hidden="false" profileTypeId="1960-ca8e-67ce-2014" profileTypeName="Unit">
