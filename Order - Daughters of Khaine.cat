              <rules/>
              <infoLinks/>
              <modifiers/>
              <characteristics>
                <characteristic name="Ability Details" characteristicTypeId="d4dc-8e81-bc0e-b8f0" value="If a target is hit by the Gaze of Morathi, pick a model in the target unit and roll a dice. If the result exceeds that model’s Wounds characteristic, it is slain."/>
              </characteristics>
            </profile>
