          <profiles>
            <profile id="451a-90d9-7de8-0317" name="Covenant of the Iron Heart" publicationId="388b-16d3-pubN65537" page="55" hidden="false" typeId="eed7-4131-0a52-0668" typeName="Prayer">
              <characteristics>
                <characteristic name="Description" typeId="0746-6cfb-5e15-53cb">Pick a friendly DAUGHTERS OF KHAINE unit within 14&quot; of the priest. Until the start of your next hero phase, you do not need to take battleshock tests for that unit.</characteristic>
              </characteristics>
            </profile>
          </profiles>
          <costs>
            <cost name="pts" typeId="points" value="0.0"/>
